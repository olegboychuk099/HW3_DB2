main
    include header
    .content
        form.filters
            .filters__filter
                p.filters__filter--text location
                img.filter__location(src="../img/location.svg")
                select.filter__select
                    option
            .filters__filter
                p.filters__filter--text category
                select.filter__select
                    option Category
            .filters__filter
                p.filters__filter--text subcategory
                select.filter__select
                    option Subcategory
            .filters__filter
                p.filters__filter--text service
                select.filter__select
                    option 3 services
            .filters__filter
                p.filters__filter--text period
                input.filter__select
            button.filters__button SEARCH
